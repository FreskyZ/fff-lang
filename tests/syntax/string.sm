
// string manipulation

fn main() {

    var string input = read_string();
    writeln("length = " + input.length);
    var string input2 = read_string();
    writeln(input == input2);

    input.push_char('a');
    input.push_char('c');
    input2.push_string("ac");
    writeln(input == input2);

    writeln(input.starts_with("abc"));
    writeln(input2.ends_with("ac"));

    var string input3 = read_string();
    const string input3_copy = input3_copy;
    writeln("input3.trim is " + input3.trim());
    writeln("input3_copy is " + input3_copy);
}