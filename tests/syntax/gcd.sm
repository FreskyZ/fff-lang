
// GCD

fn main() {

    loop "main" {
        var i32 a = read_i32();
        var i32 b = read_i32();

        if a == 0 && b == 0 {
            break "main";
        }

        println("gcd(" + a.to_string() + ", " + b.to_string() + ") is " + gcd(a, b));
    }
}

fn gcd(i32 a, i32 b) -> i32 {
    if b == 0 {
        return a;
    }
    if a < b {
        return gcd(b, a);
    } else {
        return gcd(b, a % b);
    }
}