
// GCD

fn main() {

    loop "main" {
        writeln("Input 2 numbers");
        var i32 a = read_i32();
        var i32 b = read_i32();

        if a == 0 && b == 0 {
            break "main";
        }

        writeln("最大公约数(" + a.to_string() + ", " + b.to_string() + ") is " + 最大公约数(a, b).to_string());
    }
}

fn 最大公约数(i32 a, i32 b) -> i32 {
    if b == 0 {
        return a;
    }
    if a < b {
        return 最大公约数(b, a);
    } else {
        return 最大公约数(b, a % b);
    }
}