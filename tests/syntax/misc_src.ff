fn fn1(a: u32, b: u32) -> u32 { 
    return a + b; 
} 
fn main(argc: u32, argv: [string]) -> u32 { 
    var i: u32 = 1; 
    while(i != 0) { 
        const a = read_u32(); 
        const b: u32 = read_u32(); 
        println(fn1(a, b)); 
    } 
