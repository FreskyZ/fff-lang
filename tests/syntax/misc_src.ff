// loop and name
fn main(b: u32) { 
    const a = read_i32(); 
    @abc: loop { 
        var b = 0; 
        @def: loop { 
            if a == 0 { 
                a += 1; 
                break @def; 
            } else { 
                b += 1; 
                break @abc; 
            } 
        } 
    } 
}

