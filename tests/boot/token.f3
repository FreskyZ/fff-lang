
type Span {
    row: u32,
    col: u32,
}

// without ADT or union, you have to do this, at least you must have a C style enum
enum TokenType {
    StringLiteral,
    NumericLiteral,
    Identifier,
    Operator,
}

type Token {
    ty: TokenType,
    span: Span,
    str_val: string,
    u64_val: u64,
}
