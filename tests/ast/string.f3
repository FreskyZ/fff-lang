
// string manipulation

fn main() {

    var input = read_string();
    writeln("length = " + input.length);
    var input2: string = read_string();
    println(input == input2);

    input.push_char('a');
    input.push_char('c');
    input2.push_string("ac");
    println(input == input2);

    println(input.starts_with("abc"));
    println(input2.ends_with("ac"));

    var input3 = read_string();
    const input3_copy = input3_copy;
    println("input3.trim is " + input3.trim());
    println("input3_copy is " + input3_copy);
}