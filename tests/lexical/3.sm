use std::range;
use std::io::print;
use std::io::println;

fn is_prime(num: i32) -> bool {

    for i in range(2, num / 2) {
        if num % i == 0 {
            return false;
        }
    }
    return true;
}

fn get_primes(range: i32) -> vector<i32> {
    
    let vector<i32> ret_val = vector<i32>::new();
    for i in range(3, range) {
        if is_prime(i) {
            ret_val.push_back(i);
        }
    }
    return ret_val;
}

fn main() {

    print("Primes before 100: ");
    for prime in get_primes(100) {
        println(prime);
    }
    std::process::exit(0);
}